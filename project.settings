{
    "ace": {
        "@tabSize": 2
    },
    "build": {
        "@saveall": true
    },
    "language": {
        "tern_defs": {
            "json()": {
                "browser": {
                    "enabled": true
                },
                "ecma5": {
                    "enabled": true
                },
                "jQuery": {
                    "enabled": true
                }
            }
        }
    },
    "run": {
        "configs": {
            "@inited": "true",
            "json()": {
                "run": {
                    "command": "run.sh",
                    "default": true,
                    "name": "run",
                    "runner": "Shell script",
                    "toolbar": true
                }
            }
        }
    }
}
